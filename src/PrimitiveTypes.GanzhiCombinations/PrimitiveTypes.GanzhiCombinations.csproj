<Project Sdk="Microsoft.NET.Sdk">
	<PropertyGroup>
		<TargetFramework>net8.0</TargetFramework>
		<ImplicitUsings>enable</ImplicitUsings>
		<Nullable>enable</Nullable>
		
		<AssemblyName>YiJingFramework.PrimitiveTypes.GanzhiCombinations</AssemblyName>
		<RootNamespace>$(AssemblyName)</RootNamespace>
		<GenerateDocumentationFile>True</GenerateDocumentationFile>
		<IsTrimmable>true</IsTrimmable>

		<Version>2.0.1</Version>
		<PackageId>$(AssemblyName)</PackageId>
		<PackageProjectUrl>https://github.com/YiJingFramework/PrimitiveTypes.GanzhiCombinations/wiki</PackageProjectUrl>
		<RepositoryUrl>https://github.com/YiJingFramework/PrimitiveTypes.GanzhiCombinations.git</RepositoryUrl>
		<Description>此包提供了一些类型，以表示干支的组合。 This package provides some types that represent the combinations of Tiangans and Dizhis.</Description>
		<GeneratePackageOnBuild>true</GeneratePackageOnBuild>
		<PackageRequireLicenseAcceptance>False</PackageRequireLicenseAcceptance>
		<PackageLicenseExpression>MIT</PackageLicenseExpression>
		<Authors>yueyinqiu</Authors>
		<Company>YiJingFramework</Company>
		<PackageIcon>Taichi.png</PackageIcon>
		<RepositoryType>git</RepositoryType>
		<PackageTags>YiJingFramework;天干;Tiangan;HeavenlyStem;CelestialStem;地支;Dizhi;EarthlyBranch;TerrestrialBranch;干支;Ganzhi</PackageTags>
		<PackageReadmeFile>README.md</PackageReadmeFile>
	</PropertyGroup>

	<ItemGroup>
		<None Include="..\Taichi.png">
			<Pack>True</Pack>
			<PackagePath>\</PackagePath>
		</None>
		<None Include="..\..\README.md">
			<Pack>True</Pack>
			<PackagePath>\</PackagePath>
		</None>
	</ItemGroup>

	<ItemGroup>
	  <PackageReference Include="YiJingFramework.PrimitiveTypes" Version="4.0.1" />
	</ItemGroup>
</Project>
